test(a, b, c) {
  return a+b*c;
}

fact(n){
  if n<=1 {
    return 1;
  } else {
    return n*fact(n-1);
  }
}

fizzbuzz(n) {
  var i;

  i := 1;

  while i<=n {
    if i%15 == 0 {
      write 0;
    } else if i%3 == 0 {
      write 1;
    } else if i%5 == 0 {
      write 2;
    } else {
      write 3;
    }

    i := i+1;
  }
}

main {
  var n;

  read n;
  write fact(n);
  writeln;

  fizzbuzz(10);

  write -1;
}
